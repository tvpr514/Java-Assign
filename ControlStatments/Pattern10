/* Pattern
1

10

101

1010

10101

*/



public class Pattern10 {
	public static void main(String args[]) {
		int n=5;
		for (int j=0;j<=n;j++) {
			int i=1;	
			while(i<=j) {
				if(i%2==0) {
					System.out.print("0");
				}
				else {
					System.out.print("1");
				}
				i++;
			}
			System.out.println("\n");
		}
	}
}
